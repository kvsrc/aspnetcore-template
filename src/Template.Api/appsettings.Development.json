{
  "Logging": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "WriteTo": {
      "1": {
        "Name": "Console",
        "Args": {
          "OutputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fffffff}] [{Level:u3}] [{ThreadId:0000}] [{SourceContext}] {Scope:l}{Message:l}{NewLine}{Exception}"
        }
      }
    }
  }
}
